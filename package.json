{
  "name": "backend",
  "version": "1.0.1",
  "description": "Backend for Songs V2",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "prebuild": "del /q dist",
    "build": "tsc && esbuild src/index.ts --bundle --minify --sourcemap --platform=node --target=esnext --outfile=dist/index.js",
    "postbuild": "cd dist && 7z a -tzip index.zip index.js*",
    "start": "node dist/index.js",
    "build-start": "npm run build && npm run start",
    "lint": "eslint --fix --ext .js,.ts .",
    "format": "prettier --ignore-path .gitignore -w src/**/*",
    "lint-format": "npm run lint && npm run format",
    "dev": "ts-node-dev --respawn --pretty --transpile-only src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Church-Life-Apps/Backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Church-Life-Apps/Backend/issues"
  },
  "homepage": "https://github.com/Church-Life-Apps/Backend#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "esbuild": "0.19.8",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.130",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.3",
    "@types/pg": "^8.10.9",
    "@types/pg-pool": "^2.0.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "pre-commit": "^1.2.2",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
